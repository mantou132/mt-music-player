{
  "name": "mt-music-player",
  "version": "0.0.2",
  "description": "A simply web music player app.",
  "main": "fe/index.js",
  "module": "fe/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --ignore fe/ --inspect --experimental-modules --es-module-specifier-resolution=node ./server/index.js",
    "start": "node --experimental-modules --es-module-specifier-resolution=node ./server/index.js",
    "lint": "eslint ./ --ext .js,.mjs",
    "build:fe": "NODE_ENV=production webpack",
    "start:fe": "NODE_ENV=development webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "webcomponent",
    "musicplayer",
    "pwa",
    "webapp"
  ],
  "repository": {
    "url": "https://github.com/mantou132/mt-music-player.git"
  },
  "author": "mantou",
  "license": "ISC",
  "dependencies": {
    "@types/sequelize": "^4.28.3",
    "body-parser": "^1.18.3",
    "config": "^2.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.4",
    "ioredis": "^4.0.2",
    "multer": "^1.4.1",
    "music-metadata": "^3.1.3",
    "nodemon": "^1.18.10",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.3.0",
    "upyun": "^3.3.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "sequelize-cli": "^5.2.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "engines": {
    "node": ">=12"
  }
